    "version": "0.2.0",
    "configurations": [
       
        {
            "name": "debug", //修改后
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/${workspaceFolderBasename}.exe", //写完整路径
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}", //工作目录，项目根目录
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "E:\\some_download2\\Qt_c++\\Tools\\mingw1120_64\\bin\\g++.exe", //选择MinGW中的gdb
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make-debug", //这跟depend[]很像啊，make就行了，用make编译出exe
        }
    ]
}
